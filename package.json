{
  "name": "bongocat-buddy",
  "displayName": "Bongo Cat Buddy",
  "description": "Creates a bongo cat to code with you üê±",
  "icon": "media/bongo_icon.png",
  "publisher": "JohnHarrison",
  "version": "1.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnnyRacket/bongocat-vscode-ext"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:bongocat-buddy.bongoCat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bongocat-buddy.bongoCat",
        "title": "Bongo Cat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
